{
    "name": "gameindots",
    "version": "1.2.0",
    "private": true,
    "scripts": {
        "build": "rm -rf build && parcel build src/index.html -d build --public-url ./",
        "dev": "parcel --port 8000 src/index.html -d __build",
        "test:lint": "MODE=test eslint src --ext js,jsx",
        "deploy": "gh-pages --remote origin -d build",
        "serve": "serve -s build"
    },
    "pre-commit": [
        "test:lint"
    ],
    "dependencies": {
        "prop-types": "^15.6.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "react-hot-loader": "^4.6.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.0",
        "@babel/core": "^7.5.0",
        "@babel/plugin-proposal-class-properties": "^7.5.0",
        "@babel/plugin-proposal-object-rest-spread": "^7.5.2",
        "@babel/plugin-transform-regenerator": "^7.4.5",
        "@babel/plugin-transform-runtime": "^7.5.0",
        "@babel/preset-env": "^7.5.2",
        "autoprefixer": "^9.6.1",
        "babel-eslint": "^10.0.2",
        "eslint": "^5.16.0",
        "eslint-config-frontend": "github:88Maxwell/eslint-config-frontend",
        "eslint-loader": "^2.1.1",
        "eslint-plugin-fetch": "^0.0.1",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-more": "^0.3.2",
        "eslint-plugin-no-require-lodash": "1.1.0",
        "eslint-plugin-prefer-spread": "^1.0.3",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-security": "^1.4.0",
        "gh-pages": "^2.0.1",
        "parcel-bundler": "^1.12.3",
        "postcss-modules": "^1.4.1",
        "pre-commit": "^1.2.2",
        "serve": "^11.1.0",
        "stylelint": "^9.9.0",
        "stylelint-config-standard": "^18.2.0"
    }
}
